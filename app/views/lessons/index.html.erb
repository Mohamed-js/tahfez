<p id="notice"><%= notice %></p>

<h1>Lessons</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Teacher</th>
      <th>Content type</th>
      <th>Content name</th>
      <th>From</th>
      <th>To</th>
      <th>Material</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lessons.each do |lesson| %>
      <tr>
        <td><%= lesson.student_id_id %></td>
        <td><%= lesson.teacher_id_id %></td>
        <td><%= lesson.content_type %></td>
        <td><%= lesson.content_name %></td>
        <td><%= lesson.from %></td>
        <td><%= lesson.to %></td>
        <td><%= lesson.material_id_id %></td>
        <td><%= link_to 'Show', lesson %></td>
        <td><%= link_to 'Edit', edit_lesson_path(lesson) %></td>
        <td><%= link_to 'Destroy', lesson, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lesson', new_lesson_path %>
